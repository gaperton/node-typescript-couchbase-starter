{
  "name": "typescript-couchbase-starter",
  "version": "1.0.0",
  "description": "NodeJS TypeScript Couchbase project starter",
  "main": "dist/index.js",
  "scripts": {
    "build:client": "node_modules/.bin/webpack --config ./frontend/webpack.config.js --mode production",
    "watch:client": "node_modules/.bin/webpack --config ./frontend/webpack.config.js --mode development",
    "build:server": "node_modules/.bin/tsc --project backend",
    "server": "npm run build:server && node ./dist/index.js",
    "test": "jest --forceExit --coverage --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaperton/node-typescript-couchbase-starter.git"
  },
  "keywords": [
    "nodejs",
    "node",
    "typescript",
    "couchbase"
  ],
  "author": "Vlad Balin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaperton/node-typescript-couchbase-starter/issues"
  },
  "homepage": "https://github.com/gaperton/node-typescript-couchbase-starter#readme",
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-router": "^7.0.31",
    "@types/koa-session": "^5.7.4",
    "@types/koa-static": "^4.0.0",
    "@types/react-dom": "^16.0.7",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "style-loader": "^0.23.0",
    "ts-loader": "^5.1.0",
    "typescript": "^3.0.3",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "couchbase": "^2.5.1",
    "dotenv": "^6.0.0",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-session": "^5.9.0",
    "koa-static": "^5.0.0"
  }
}
